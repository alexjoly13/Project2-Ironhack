<h2>Log In</h2>

{{> flash_messages}}

<form action="/process-login" method="POST">
  <label>
    Email:
    <input type="email" name="email" placeholder="qira@solo.com" required>
  </label>

  <label>
    Password:
    <input type="password" name="originalPassword" placeholder="Your password" required>
  </label>

  <button>Log In</button>
</form>